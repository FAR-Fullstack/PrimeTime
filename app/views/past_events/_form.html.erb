<%= simple_form_for @past_event do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :date_start %>
  <%= f.input :date_end %>
  <%= f.input :location %>
  <%= f.input :category, :as => :check_boxes, :collection => ["Public Event", "Private Event"] %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit%>
<% end %>
